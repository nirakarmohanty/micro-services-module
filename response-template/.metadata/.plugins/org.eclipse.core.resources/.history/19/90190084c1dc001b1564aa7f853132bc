package src.coding.tutor.patient.service.entity;

import javax.persistence.Entity;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@Entity
@AllArgsConstructor
@NoArgsConstructor
public class Patient {

	private long id;
	private String name;
	private int age;
	private String sex;
	
}
